<controls>
  <general>
    <name>MS1 with Spalart-Allmaras</name>
    <description>Cartesian Stretched</description>
    <caseid>MS Base</caseid>
    <material>AIR</material>
    <referenceMaterial>AIR</referenceMaterial>
    <outFileName>output.out</outFileName>
    <nsave>-1</nsave>
  </general>
  <materials>
    <material name="AIR">
      <fluid>
        <viscosityMolecular>1.000000E-07</viscosityMolecular>
        <density>1.000000E+00</density>
      </fluid>
    </material>
  </materials>
  <accelerationOfGravity>
  </accelerationOfGravity>
  <grids>
    <grid>
      <gridFilePath>../../grids_refresco</gridFilePath>
      <gridFileName>grid_ist_curved_051_refresco</gridFileName>
    </grid>
  </grids>
  <boundaries>
    <family name="Symmetry">
      <BCSymmetryPlane>
      </BCSymmetryPlane>
    </family>
    <family name="Wall">
      <BCWall>
        <useWallFunction>NO</useWallFunction>
        <roughnessModel>
          <NONE>
          </NONE>
        </roughnessModel>
      </BCWall>
    </family>
    <family name="FreeSlip">
      <BCAllDirichlet>
        <turbulence>
          <eddyVisc>
          </eddyVisc>
        </turbulence>
      </BCAllDirichlet>
    </family>
    <family name="Inflow">
      <BCInflow>
        <checkFlux>false</checkFlux>
        <turbulence>
          <eddyVisc>
          </eddyVisc>
        </turbulence>
      </BCInflow>
    </family>
    <family name="Outflow">
      <BCAllDirichlet>
        <turbulence>
          <eddyVisc>
          </eddyVisc>
        </turbulence>
      </BCAllDirichlet>
    </family>
  </boundaries>
  <bodies>
  </bodies>
  <coprocessing>
  </coprocessing>
  <gridquality>
    <saveMetrics>apply_correction_f_c</saveMetrics>
  </gridquality>
  <timeLoop>
  </timeLoop>
  <outerLoop>
    <maxIteration>100</maxIteration>
    <convergenceToleranceLinf>1.000000E+09</convergenceToleranceLinf>
    <convergenceToleranceL2>1.000000E-11</convergenceToleranceL2>
  </outerLoop>
  <restart>
  </restart>
  <massMomentumSolver>
    <AndersonAcceleration>
      <apply>true</apply>
      <frequency>1</frequency>
    </AndersonAcceleration>
    <solverType>
      <segregated>
      </segregated>
    </solverType>
  </massMomentumSolver>
  <equations>
    <equation name="mom">
      <EQMomentum>
        <solve_z>false</solve_z>
        <solver>
          <MARIN>
          </MARIN>
        </solver>
        <maxIteration>500</maxIteration>
        <relaxationProcedure>
          <IMPLICIT_EXPLICIT>
            <imp_relax_min>9.900000E-01</imp_relax_min>
            <imp_relax_max>9.900000E-01</imp_relax_max>
            <imp_relax_fac>500</imp_relax_fac>
          </IMPLICIT_EXPLICIT>
        </relaxationProcedure>
        <convectiveFluxDiscretisation>
          <KAPPA_SCHEME>
            <schemeName>QUICK</schemeName>
          </KAPPA_SCHEME>
        </convectiveFluxDiscretisation>
        <gradientCalculation>
          <GAUSS>
          </GAUSS>
        </gradientCalculation>
        <limitEccentricityCorrection>false</limitEccentricityCorrection>
        <userSource>true</userSource>
        <saveSkinFriction>false</saveSkinFriction>
        <initialization>
          <USER_DEFINED>
            <initialVelocity userCode="true">1.000000E+00  0.000000E+00  0.000000E+00</initialVelocity>
          </USER_DEFINED>
        </initialization>
      </EQMomentum>
    </equation>
    <equation name="pres">
      <EQPressure>
        <solver>
          <PETSC>
            <solver>GMRES</solver>
          </PETSC>
        </solver>
        <relaxationProcedure>
          <EXPLICIT>
          </EXPLICIT>
        </relaxationProcedure>
        <convectiveFluxDiscretisation>
          <FOU_CDS_BLEND>
          </FOU_CDS_BLEND>
        </convectiveFluxDiscretisation>
        <gradientCalculation>
          <GAUSS>
          </GAUSS>
        </gradientCalculation>
        <pressureReferencePosition>5.000000E-01  5.000000E-01  0.000000E+00</pressureReferencePosition>
        <userSource>true</userSource>
      </EQPressure>
    </equation>
    <equation name="turb">
      <EQTurbulence>
        <turbulenceModel>
          <SPALART_ALLMARAS>
          </SPALART_ALLMARAS>
        </turbulenceModel>
        <solver>
          <MARIN>
          </MARIN>
        </solver>
        <convergenceTolerance>1.000000E-01</convergenceTolerance>
        <maxIteration>0</maxIteration>
        <relaxationProcedure>
          <IMPLICIT_EXPLICIT>
            <imp_relax_min>9.950000E-01</imp_relax_min>
            <imp_relax_max>9.950000E-01</imp_relax_max>
            <imp_relax_fac>500</imp_relax_fac>
          </IMPLICIT_EXPLICIT>
        </relaxationProcedure>
        <relaxEddyViscosity>0.000000E+00</relaxEddyViscosity>
        <stagnationRegionCorrection>
          <NONE>
          </NONE>
        </stagnationRegionCorrection>
        <convectiveFluxDiscretisation>
          <KAPPA_SCHEME>
            <schemeName>QUICK</schemeName>
          </KAPPA_SCHEME>
        </convectiveFluxDiscretisation>
        <transportEquations>
        </transportEquations>
        <gradientCalculation>
          <GAUSS>
          </GAUSS>
        </gradientCalculation>
        <limitEccentricityCorrection>false</limitEccentricityCorrection>
        <multiphaseTurbulenceModification>
          <NONE>
          </NONE>
        </multiphaseTurbulenceModification>
        <eddyViscosityCorrection>
          <NONE>
          </NONE>
        </eddyViscosityCorrection>
        <decayControl>
          <NONE>
          </NONE>
        </decayControl>
        <userSource>true</userSource>
        <saveResidual>true</saveResidual>
        <initialization>
          <USER_DEFINED>
            <eddyVisc>
            </eddyVisc>
          </USER_DEFINED>
        </initialization>
      </EQTurbulence>
    </equation>
  </equations>
  <extraQuantities>
  </extraQuantities>
  <monitors>
    <monitor name="Forces">
      <MO_Force>
        <fileName>post</fileName>
        <saveFrequency>50</saveFrequency>
        <families>Wall</families>
      </MO_Force>
    </monitor>
  </monitors>
  <deformGrid>
  </deformGrid>
  <adaptiveGrid>
  </adaptiveGrid>
  <freeMotions>
  </freeMotions>
  <fsi>
  </fsi>
  <acoustics>
    <general>
    </general>
  </acoustics>
  <particleTracking>
    <general>
    </general>
    <flowfieldInterpolation>
      <interpolation>
        <NEAREST_CELL>
        </NEAREST_CELL>
      </interpolation>
    </flowfieldInterpolation>
    <wallInteractions>
    </wallInteractions>
    <modelling>
      <symmetryPlane>
      </symmetryPlane>
      <solutionBox>
      </solutionBox>
    </modelling>
    <numericalIntegration>
    </numericalIntegration>
    <bubbleNoise>
    </bubbleNoise>
  </particleTracking>
  <buoyancyBoussinesq>
  </buoyancyBoussinesq>
  <externalForces>
    <connectionPoints>
    </connectionPoints>
    <externalForceModels>
    </externalForceModels>
  </externalForces>
  <xmfCoupling>
    <couplingStrategies>
    </couplingStrategies>
  </xmfCoupling>
  <turbulenceGeneration>
    <general>
    </general>
  </turbulenceGeneration>
  <bodyForces>
    <bodyForceModels>
    </bodyForceModels>
  </bodyForces>
  <dynamic>
    <scenario>
    </scenario>
  </dynamic>
  <developer>
    <faceInterpolation_nearwallCorrection>1.000000E+07</faceInterpolation_nearwallCorrection>
    <faceInterpolation_check_projectednodes>true</faceInterpolation_check_projectednodes>
    <warnForInefficientCellsPerCore>false</warnForInefficientCellsPerCore>
  </developer>
  <overset>
    <oversetSetup>
      <dci>
        <SUGGAR_DCI>
        </SUGGAR_DCI>
      </dci>
      <interpolationReFRESCO>
        <interpolation>
          <NEAREST_CELL>
          </NEAREST_CELL>
        </interpolation>
      </interpolationReFRESCO>
    </oversetSetup>
  </overset>
  <precice_adapter>
    <preciceSetup>
      <mapping>
        <rbf_compact_tps_c2>
        </rbf_compact_tps_c2>
      </mapping>
      <couplingScheme>
        <method>
          <serialExplicit>
          </serialExplicit>
        </method>
      </couplingScheme>
      <accelerationMethod>
        <method>
          <constantRelaxation>
          </constantRelaxation>
        </method>
      </accelerationMethod>
      <watchPoints>
      </watchPoints>
    </preciceSetup>
  </precice_adapter>
</controls>
